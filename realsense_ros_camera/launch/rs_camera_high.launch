<launch>
  <arg name="is_435" default="false"/>
  <arg name="high_density" default="false"/>
  <arg name="mid_density" default="false"/>
  <arg name="high_accuracy" default="false"/>

  <arg name="serial_no" value="752112070754" if="$(arg is_435)"/>
  <arg name="namespace" value="realsense435" if="$(arg is_435)"/>
  <arg name="base_frame_id" value="realsense_435_link" if="$(arg is_435)"/>
  <arg name="depth_optical_frame_id" default="realsense_435_depth_optical_frame"  if="$(arg is_435)"/>
  <arg name="color_optical_frame_id" default="realsense_435_color_optical_frame"  if="$(arg is_435)"/>

  <arg name="serial_no" value="739112060392" unless="$(arg is_435)"/>
  <arg name="namespace" value="realsense415" unless="$(arg is_435)"/>
  <arg name="base_frame_id" value="realsense_415_link" unless="$(arg is_435)"/>
  <arg name="depth_optical_frame_id" default="realsense_415_depth_optical_frame"  unless="$(arg is_435)"/>
  <arg name="color_optical_frame_id" default="realsense_415_color_optical_frame"  unless="$(arg is_435)"/>

  <arg name="operation_mode" value="$(find realsense_ros_camera)/launch/includes/high_res_high_density.json" if="$(arg high_density)"/>
  <arg name="operation_mode" value="$(find realsense_ros_camera)/launch/includes/high_res_mid_density.json" if="$(arg mid_density)"/>
  <arg name="operation_mode" value="$(find realsense_ros_camera)/launch/includes/high_res_high_accuracy.json" if="$(arg high_accuracy)"/>

  <arg name="fisheye_width"       default="1280"/>
  <arg name="fisheye_height"      default="720"/>
  <arg name="enable_fisheye"      default="true"/>

  <arg name="depth_width"         default="1280"/>
  <arg name="depth_height"        default="720"/>
  <arg name="enable_depth"        default="true"/>

  <arg name="infra1_width"        default="1280"/>
  <arg name="infra1_height"       default="720"/>
  <arg name="enable_infra1"       default="false"/>

  <arg name="infra2_width"        default="1280"/>
  <arg name="infra2_height"       default="720"/>
  <arg name="enable_infra2"       default="false"/>

  <arg name="color_width"         default="1280"/>
  <arg name="color_height"        default="720"/>
  <arg name="enable_color"        default="true"/>

  <arg name="fisheye_fps"         default="15"/>
  <arg name="depth_fps"           default="15"/>
  <arg name="infra1_fps"          default="15"/>
  <arg name="infra2_fps"          default="15"/>
  <arg name="color_fps"           default="15"/>
  <arg name="gyro_fps"            default="1000"/>
  <arg name="accel_fps"           default="1000"/>
  <arg name="enable_imu"          default="true"/>

  <arg name="enable_pointcloud"   default="false"/>
  <arg name="enable_sync"         default="false"/>
  <arg name="align_depth"         default="false"/>

  <group ns="$(arg namespace)">
    <include file="$(find realsense_ros_camera)/launch/includes/nodelet.launch.xml">
      <arg name="serial_no"                value="$(arg serial_no)"/>
      <arg name="json_file_path"           value="$(arg operation_mode)"/>
      <arg name="base_frame_id"            value="$(arg base_frame_id)"/>

      <arg name="enable_pointcloud"        value="$(arg enable_pointcloud)"/>
      <arg name="enable_sync"              value="$(arg enable_sync)"/>
      <arg name="align_depth"              value="$(arg align_depth)"/>

      <arg name="fisheye_width"            value="$(arg fisheye_width)"/>
      <arg name="fisheye_height"           value="$(arg fisheye_height)"/>
      <arg name="enable_fisheye"           value="$(arg enable_fisheye)"/>

      <arg name="depth_width"              value="$(arg depth_width)"/>
      <arg name="depth_height"             value="$(arg depth_height)"/>
      <arg name="enable_depth"             value="$(arg enable_depth)"/>

      <arg name="color_width"              value="$(arg color_width)"/>
      <arg name="color_height"             value="$(arg color_height)"/>
      <arg name="enable_color"             value="$(arg enable_color)"/>

      <arg name="infra1_width"             value="$(arg infra1_width)"/>
      <arg name="infra1_height"            value="$(arg infra1_height)"/>
      <arg name="enable_infra1"            value="$(arg enable_infra1)"/>

      <arg name="infra2_width"             value="$(arg infra2_width)"/>
      <arg name="infra2_height"            value="$(arg infra2_height)"/>
      <arg name="enable_infra2"            value="$(arg enable_infra2)"/>

      <arg name="fisheye_fps"              value="$(arg fisheye_fps)"/>
      <arg name="depth_fps"                value="$(arg depth_fps)"/>
      <arg name="infra1_fps"               value="$(arg infra1_fps)"/>
      <arg name="infra2_fps"               value="$(arg infra2_fps)"/>
      <arg name="color_fps"                value="$(arg color_fps)"/>
      <arg name="gyro_fps"                 value="$(arg gyro_fps)"/>
      <arg name="accel_fps"                value="$(arg accel_fps)"/>
      <arg name="enable_imu"               value="$(arg enable_imu)"/>
    </include>
  </group>
</launch>
